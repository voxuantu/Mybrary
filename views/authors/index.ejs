<h2>All Authors</h2>
<form action="/authors" method="get">
    <label for="name">Name</label>
    <input type="text" name="name" id="name" value="<%= searchOption.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% authors.forEach(author =>{ %>
    <div><%= author.name%></div>
<% })%>